# Development Environment Variables
# Copy this file to .env.development and update the values

# PostgreSQL connection string for local Supabase CLI
SUPABASE_CONNECTION_STRING=postgresql://postgres:postgres@localhost:54322/postgres

# Supabase configuration
SUPABASE_PUBLIC_URL=http://localhost:54321
SERVICE_ROLE_KEY=your-service-role-key-here
ANON_KEY=your-anon-key-here

# Other service URLs
ANALYTICS_BASE_URL=http://localhost:4000
RETRIEVAL_BASE_URL=http://localhost:3001

# Database schema
DB_SCHEMA=public
