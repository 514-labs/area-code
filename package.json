{
  "name": "with-vite",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "services/*"
  ],
  "scripts": {
    "dev": "turbo run dev:ufa --ui=tui",
    "build": "turbo run build",
    "clean": "turbo run clean",
    "dev:clean": "pnpm --filter @workspace/transactional-database dev:clean && pnpm --filter @workspace/retrieval-base dev:clean && pnpm --filter sync-base dev:clean",
    "dev:seed": "./scripts/dev-seed.sh"
  },
  "devDependencies": {
    "@turbo/gen": "^1.13.4",
    "eslint": "^8.57.1",
    "prettier": "^3.6.2",
    "turbo": "^2.5.4",
    "typescript": "^5.8.3"
  },
  "packageManager": "pnpm@9.9.0",
  "engines": {
    "node": ">=18"
  }
}
