{
  "name": "area-code",
  "version": "0.3.0",
  "private": true,
  "workspaces": [
    "ufa/apps/*",
    "ufa/packages/*",
    "ufa/services/*",
    "odw/apps/*",
    "odw/packages/*",
    "odw/services/*"
  ],
  "scripts": {
    "ufa:dev": "turbo run ufa:dev --ui=tui --filter='!@workspace/transactional-sqlserver'",
    "ufa:dev:clean": "turbo run ufa:dev:clean",
    "ufa:dev:seed": "./ufa/scripts/dev-seed.sh",
    "ufa:dev:seed:transactional-supabase-foobar": "./ufa/scripts/dev-seed.sh --service=transactional-supabase-foobar",
    "ufa:dev:seed:analytical-moose-foobar": "./ufa/scripts/dev-seed.sh --service=analytical-moose-foobar",
    "ufa:dev:seed:retrieval-elasticsearch-foobar": "./ufa/scripts/dev-seed.sh --service=retrieval-elasticsearch-foobar",
    "ufa:sqlserver:dev": "turbo run ufa:dev --ui=tui --filter='!transactional-supabase-foobar' --filter='!sync-supabase-moose-foobar'",
    "ufa:sqlserver:dev:seed": "turbo run ufa:dev:seed --filter='@workspace/transactional-sqlserver'"
  },
  "devDependencies": {
    "@turbo/gen": "^1.13.4",
    "eslint": "^8.57.1",
    "prettier": "^3.6.2",
    "turbo": "^2.5.4",
    "typescript": "^5.8.3"
  },
  "packageManager": "pnpm@9.9.0",
  "engines": {
    "node": ">=18"
  }
}
