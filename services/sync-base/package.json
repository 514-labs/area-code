{
  "name": "sync-base",
  "version": "0.0",
  "scripts": {
    "moose": "moose-cli",
    "build": "moose-cli build --docker",
    "dev": "moose-cli dev",
    "dev:ufa": "moose-cli dev",
    "dev:clean": "moose-cli clean",
    "dev:workflow": "./scripts/wait-and-start-workflow.sh",
    "dev:workflow:stop": "moose-cli workflow terminate supabase-listener && cd ../transactional-database && pnpm replication:stop",
    "dev:workflow:start": "cd ../transactional-database && pnpm setup:replication && cd ../sync-base && moose-cli workflow run supabase-listener",
    "dev:workflow:status": "moose-cli workflow status supabase-listener",
    "dev:workflow:list": "moose-cli workflow list"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "ts-patch": "^3.3.0",
    "@514labs/moose-lib": "latest",
    "typia": "^7.6.0",
    "@supabase/supabase-js": "^2.39.0",
    "dotenv": "^16.3.1",
    "@workspace/models": "workspace:*"
  },
  "devDependencies": {
    "@514labs/moose-cli": "latest",
    "@types/node": "^20.12.12"
  }
}
