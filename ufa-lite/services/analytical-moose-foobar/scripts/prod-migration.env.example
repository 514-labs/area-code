# Environment variables for ClickHouse prod migration script
# Copy this file to prod-migration.env and fill the values to skip connection prompts

# PostgreSQL connection string
# Format: postgresql://user:password@host:port/database
# Example: postgresql://postgres:mypassword@prod-db.example.com:5432/myapp
PG_CONNECTION_STRING=

# ClickHouse connection details
# Database URL (include protocol and port)
# Example: https://ch-cluster.example.com:8443 or http://localhost:8123
CH_DB_URL=

# ClickHouse username
CH_USERNAME=

# ClickHouse password
CH_PASSWORD=

# ClickHouse database name (optional - defaults to 'default')
CH_DATABASE=

# Table configuration (optional - if provided, will skip table name prompts)
SOURCE_TABLE=
DEST_TABLE=

CLEAR_DATA=

# Keep temporary files after migration (true/false, defaults to false)
KEEP_TEMP=

# Note: The script will still prompt for record count limit if table names are provided
